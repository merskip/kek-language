
func foo(a: Integer) -> Integer {
    a
}

func bar(a: Integer, b: Integer) -> Integer {
    if (a == 10) {
        foo(10)
    }
    else if (a == 20) {
        foo(20)
    }
    else if (a == 30) {
        foo(30)
    }
    else {
        foo(0)
    }
    a + b
}

func main() -> Integer {
    bar(1, 3) - 1
}
